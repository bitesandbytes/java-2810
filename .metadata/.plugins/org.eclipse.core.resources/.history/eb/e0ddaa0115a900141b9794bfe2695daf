package acadSection;

public class nonFaculty extends Employee implements CompareThem
{
	String nonFacultyID;
	String department;
	
	public nonFaculty()
	{
		super();
		department = null;
	}
	
	public nonFaculty(String nonFacultyID, String department)
	{
		super();
		this.nonFacultyID = nonFacultyID;
		this.department = department;
	}

	@Override
	public String getID()
	{
		return nonFacultyID;
	}
	
	@Override
	public void setID(String nonFacultyID)
	{
		this.nonFacultyID = nonFacultyID;
	}
		
	public String getDepartment()
	{
		return department;
	}

	public void setDepartment(String department)
	{
		this.department = department;
	}

}