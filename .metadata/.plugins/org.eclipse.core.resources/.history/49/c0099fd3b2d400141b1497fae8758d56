
public class Node<T extends Comparable<T>> implements Comparable<Node<T>>
{
	T data;
	Node<T> left, right, parent;
	int balanceFactor;
	
	Node(T data)
	{
		this.data = data;
		this.left = null;
		this.right = null;
		this.parent = null;
		balanceFactor = 0;
	}
	
	public int compareTo(Node<T> other)
	{
		return this.data.compareTo(other.data);
	}
}
