package acadSection;

public class Student extends Person 
{
	String rollNo;
	String hostel;
	double CGPA;
	int semester;
	int creditsEarned;
	int currentSemesterCreditsTotal;
	int currentSemesterCreditsEarned;
	Faculty facAd;
	
	public Student()
	{
		super();
		rollNo = null;
		hostel = null;
		CGPA = 0;
		semester = 0;
		currentSemesterCreditsEarned = 0;
		currentSemesterCreditsTotal = 0;
		facAd = null;
	}
}
