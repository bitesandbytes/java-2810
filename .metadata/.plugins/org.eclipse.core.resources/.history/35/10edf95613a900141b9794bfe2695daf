package acadSection;

public class Faculty extends Employee
{
	int facultyID;
	String profPosition;
	
	public Faculty()
	{
		super();
		facultyID = 0;
		profPosition = null;
	}
	
	public int getID()
	{
		return facultyID;
	}

	public void setID(int facultyID)
	{
		this.facultyID = facultyID;
	}

	public String getProfPosition()
	{
		return profPosition;
	}

	public void setProfPosition(String profPosition)
	{
		this.profPosition = profPosition;
	}

}
